        "@types/chai": "^4.0.10",
        "@types/compression": "0.0.35",
        "@types/helmet": "0.0.37",
        "@types/mocha": "^2.2.44",
        "@types/sinon": "^4.1.2",
        "body-parser": "^1.18.2",
        "chai": "^4.1.2",
        "compression": "^1.7.1",
