        "gulp-tslint": "^8.1.2",
        "gulp-tslint-jenkins-reporter": "^1.1.0",
        "gulp-typescript": "^3.2.3",
        "helmet": "^3.9.0",
        "inversify": "^4.5.1",
        "inversify-express-utils": "^4.2.2",
        "mocha": "^4.0.1",
