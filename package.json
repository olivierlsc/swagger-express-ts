        "typescript",
        "expressjs",
        "express",
        "api rest",
        "documentation",
        "decorator",
        "generator",
        "swagger.json",
        "json",
        "rest"
    ],
    "bugs": {
        "url": "https://github.com/olivierlsc/swagger-specification-ts/issues"
    },
    "homepage": "https://github.com/olivierlsc/swagger-specification-ts#readme",
