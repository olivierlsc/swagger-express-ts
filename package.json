{
    "name": "@mje0002/swagger-ts-decorators",
    "version": "1.0.1",
    "description": "Generate and serve swagger.json",
    "main": "index.js",
    "types": "index.d.ts",
    "typings": "index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mje0002/swagger-ts-decorators.git"
    },
    "author": "Matt Echeverria",
    "license": "MIT",
    "keywords": [
        "inversify",
        "inversifyjs",
        "swagger",
        "swagger2",
        "swagger-ui",
        "typescript",
        "expressjs",
        "express",
        "api rest",
        "documentation",
        "decorator",
        "generator",
        "swagger.json",
        "json",
        "rest"
    ],
    "bugs": {
        "url": "https://github.com/mje0002/swagger-ts-decorators/issues"
    },
    "homepage": "https://github.com/mje0002/swagger-ts-decorators#readme",
    "scripts": {
        "build": "tsc --project tsconfig.lib.json && tsc-alias -p tsconfig.lib.json",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint \"{src,test}/**/*.ts\" --fix",
        "test:unit": "npm run test",
        "test": "jest"
    },
    "peerDependencies": {
        "@types/body-parser": "^1.19.0",
        "@types/compression": "1.7.0",
        "@types/express": "^4.17.11",
        "@types/helmet": "0.0.48",
        "@types/lodash": "^4.14.168",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "express": "^4.17.1",
        "helmet": "^4.4.1",
        "lodash": "^4.17.21",
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
        "@faker-js/faker": "7.6.0",
        "@types/jest": "29.0.3",
        "@types/parallel-transform": "1.1.1",
        "@typescript-eslint/eslint-plugin": "5.21.0",
        "@typescript-eslint/parser": "5.21.0",
        "eslint": "8.14.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^29.3.1",
        "jest-mock-extended": "3.0.0-beta1",
        "rimraf": "^3.0.2",
        "prettier": "2.6.2",
        "ts-jest": "29.0.1",
        "ts-node": "10.7.0",
        "tsc-alias": "1.7.0",
        "typescript": "4.6.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "roots": [
            "test/unit",
            "<rootDir>"
        ],
        "rootDir": ".",
        "moduleNameMapper": {
            "@/(.*)": "<rootDir>/src/$1"
        },
        "testRegex": ".*\\.spec\\.ts$",
        "collectCoverageFrom": [
            "src/**/*.ts"
        ],
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "restoreMocks": true,
        "clearMocks": true,
        "resetMocks": true,
        "coverageThreshold": {
            "global": {
                "statements": 81
            }
        },
        "testMatch": null,
        "preset": "ts-jest/presets/js-with-ts",
        "setupFilesAfterEnv": [
            "<rootDir>/test/helpers/jest.setup.ts"
        ]
    },
    "dependencies": {
        "@types/express": "^4.17.15",
        "@types/lodash": "^4.14.191",
        "express": "^4.18.2",
        "lodash": "^4.17.21",
        "reflect-metadata": "^0.1.13"
    },
    "directories": {
        "test": "test"
    }
}